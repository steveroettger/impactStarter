<% provide(:title, 'Organizations') %>
<div class="interior-container">
  <h1>Organizations</h1>
  <% if @organizations.none? and params.has_key? :search -%>
    <div class="organization-container">
      <p>No organizations found for: <em>"<%= params[:search] -%>"</em></p>
    </div>
  <% end -%>
  <%= render partial: 'organization', collection: @organizations %>
</div>
